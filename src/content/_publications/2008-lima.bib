@article{Lima2008,
  author    = {Lima, Carlos Diego Quirino and Chaves, Luiz Carlos Rodrigues and Carvalho, Ygor De Oliveira de and Dantas, Herbert Anderson de Vasconcelos and Rocha, Edemberg Silva da and Lira, Heremita Brasileiro and Moura, Thiago},
  title     = {Reuso de protótipos de interface: uma proposta para utilização de tecnologias Java para web},
  abstract  = {Reutilizar recursos é fundamental para minimizar esforços no processo de desenvolvimento de software. Em geral, não é fácil encontrar indicações, referências ou guias que tratem da aplicação do reuso de protótipos, sobretudo quando desenvolve-se sistemas Web. No entanto, o avanço substancial de estudos na área, aliado às novas tecnologias Java, tornou mais simples a adoção da prototipagem e, em paralelo, permite que se antecipe a construção da interface final do sistema Web, aproveitando-a completamente na fase de implementação. Diante disso, o Núcleo de Pesquisa e Desenvolvimento do CEFET-PB, do Projeto SIEP Gerencial resolveu, aliado à análise e projeto do SIPE (Sistema de Acompanhamento de Projetos Especiais), desenvolver um protótipo de interfaces, para demonstrar a possibilidade de reutilização.},
  keywords  = {prototipação de interfaces, reutilização de protótipos de interface, tecnologias Java Web, SIEP.},
  pages     = {80--85},
  journal   = {Revista Principia - Divulgação Científica e  Tecnológica do IFPB},
  publisher = {Instituto Federal de Educacao, Ciencia e Tecnologia da Paraiba},
  volume    = {1},
  number    = {17},
  year      = {2008},
  month     = aug,
  ISSN      = {2447-9187},
  DOI       = {10.18265/1517-03062015v1n17p80-85},
  url       = {https://dx.doi.org/10.18265/1517-03062015v1n17p80-85}
}
